#include<stdio.h>

#define CUBE(x) ((x)*(x)*(x))
#define REMINDER(x) ((x)%4)
#define BOOL(x, y) ((x)*(y)<100 ? 1 : 0)

int main (void)
{
	printf("%d\n", CUBE(2+1));
	printf("%d\n", REMINDER(6));
	printf("%d", BOOL(6, 20));
	return 0;
}
