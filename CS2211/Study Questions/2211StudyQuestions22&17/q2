#include<stdio.h>
#include<stdlib.h>
#include<string.h>

void* duplicate(char*);

int main (void)
{
	puts(duplicate("Zaid is cool!"));
	return 0;
}

void* duplicate(char* str)
{
	char* ptr = malloc(sizeof(str));
	return strcpy(ptr, str);
}
