void swap (int *p, int* q);

int main (void)
{
	int x=0, y=2;
	printf("The values before the swap, x = %d, y = %d\n", x, y);
	swap(&x, &y);
	printf("The values after the swap, x = %d, y = %d", x, y);
	return 0;
}

void swap(int *p, int* q)
{
	int x=*p;

	*p=*q;
	*q=x;
}
